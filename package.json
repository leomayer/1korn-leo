{
  "name": "1korn-leo",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4641",
    "start-de": "ng serve --browser-target 1korn-leo:build:development,de --port 4642",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "extract-i18n": "ng extract-i18n && npm run merge-i18n",
    "merge-i18n": "pushd src/i18n/ && for FILE in *.[a-z][a-z].xlf ; do pomerge -t messages.xlf -i $FILE -o $FILE.new.xlf && mv $FILE.new.xlf $FILE && FILE_LANG=`echo \"$FILE\" | sed -e 's/messages.\\([a-z]*\\).*/\\1/'` && sed -i $FILE -e \"s/\\(original=\\\"ng2.template\\\"\\)>/\\1 target-language=\\\"$FILE_LANG\\\">/\"; done && popd",
    "prettier": "prettier --write \"./{src,e2e}/**/*.{ts,js,html,scss}\"",
    "prettier-watch": "onchange \"./{src,e2e}/**/*.{ts,js,html,scss}\" -- prettier --write  --ignore-unknown {{changed}}",
    "test": "ng test",
    "start-1korn": "npm run start & npm run prettier-watch",
    "github-pages": "ng deploy --base-href=/1korn-leo/"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.2.0",
    "@angular/cdk": "^14.1.0",
    "@angular/common": "^14.2.0",
    "@angular/compiler": "^14.2.0",
    "@angular/core": "^14.2.0",
    "@angular/forms": "^14.2.0",
    "@angular/localize": "^14.2.12",
    "@angular/material": "^14.1.0",
    "@angular/material-moment-adapter": "^14.2.7",
    "@angular/platform-browser": "^14.2.0",
    "@angular/platform-browser-dynamic": "^14.2.0",
    "@angular/router": "^14.2.12",
    "@trivago/prettier-plugin-sort-imports": "^3.4.0",
    "angular-resize-event": "^3.2.0",
    "angular-split": "^14.1.0",
    "async-mutex": "^0.4.0",
    "core-js": "^3.26.1",
    "file-saver": "^2.0.5",
    "json-server": "^0.17.1",
    "ng-table-virtual-scroll": "^1.4.1",
    "ng2-file-upload": "^1.4.0",
    "onchange": "^7.1.0",
    "postcss": "^8.4.20",
    "prettier-plugin-css-order": "^1.3.0",
    "prettier-plugin-organize-attributes": "^0.0.5",
    "roboto-fontface": "^0.10.0",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "xlf-merge": "^2.0.1",
    "xlsx": "^0.18.5",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.10",
    "@angular-eslint/builder": "^14.4.0",
    "@angular-eslint/eslint-plugin": "^14.0.2",
    "@angular-eslint/eslint-plugin-template": "^14.0.2",
    "@angular-eslint/schematics": "^14.4.0",
    "@angular-eslint/template-parser": "^14.4.0",
    "@angular/cli": "~14.2.10",
    "@angular/compiler-cli": "^14.2.0",
    "@types/file-saver": "^2.0.5",
    "@types/jasmine": "~4.0.0",
    "@types/jasminewd2": "^2.0.10",
    "@types/node": "^18.11.17",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "angular-cli-ghpages": "^1.0.5",
    "eslint": "^8.30.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^39.6.4",
    "eslint-plugin-prefer-arrow": "1.2",
    "jasmine-core": "~4.3.0",
    "jasmine-spec-reporter": "^7.0.0",
    "karma": "~6.4.0",
    "karma-cli": "^2.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "npm": "^8.19.3",
    "prettier": "^2.8.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.2",
    "zone.js": "^0.11.8"
  }
}
